        <StackPanel Style="{StaticResource MainStackPanelStyle}" Grid.Row="1">

            <!-- FACEBOOK BUTTON -->
            <UserControl>
                <StackPanel HorizontalAlignment="Left">

                    <VisualStateManager.VisualStateGroups>
